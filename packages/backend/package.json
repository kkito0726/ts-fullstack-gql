    "@types/express": "4.17.17",
    "@types/glob": "8.1.0",
    "@types/node": "18.14.1",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "copyfiles": "2.4.1",
    "eslint": "8.34.0",
    "nodemon": "2.0.20",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@apollo/server": "4.4.0",
    "@prisma/client": "4.13.0",
    "express": "4.18.2",
    "glob": "10.2.2",
